{
  "dashboard": {
    "title": "PAW Oracle Module Metrics",
    "description": "Oracle module metrics including price feeds, validator participation, TWAP, and cross-chain price aggregation",
    "tags": ["paw", "oracle", "price-feeds", "validators", "twap"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "asset",
          "type": "query",
          "label": "Asset",
          "query": "label_values(paw_oracle_price_submissions_total, asset)",
          "datasource": "Prometheus",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "validator",
          "type": "query",
          "label": "Validator",
          "query": "label_values(paw_oracle_validator_submissions_total, validator)",
          "datasource": "Prometheus",
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Assets Tracked",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "paw_oracle_assets_tracked_total",
            "legendFormat": "Assets",
            "refId": "A"
          }
        ],
        "description": "Total number of price feeds being tracked",
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Active Validators",
        "gridPos": {
          "x": 4,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "count(count by (validator) (paw_oracle_validator_submissions_total))",
            "legendFormat": "Validators",
            "refId": "A"
          }
        ],
        "description": "Number of validators submitting oracle prices",
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Price Submissions (24h)",
        "gridPos": {
          "x": 8,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_price_submissions_total[24h]))",
            "legendFormat": "Submissions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Avg Price Deviation",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "avg(paw_oracle_price_deviation_percent)",
            "legendFormat": "Deviation",
            "refId": "A"
          }
        ],
        "description": "Average validator price deviation from median",
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 2,
                  "color": "yellow"
                },
                {
                  "value": 5,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Slash Events (24h)",
        "gridPos": {
          "x": 16,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_slashing_events_total[24h]))",
            "legendFormat": "Slashes",
            "refId": "A"
          }
        ],
        "description": "Validator slashing events in last 24 hours",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 10,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Outliers Detected (1h)",
        "gridPos": {
          "x": 20,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_outliers_detected_total[1h]))",
            "legendFormat": "Outliers",
            "refId": "A"
          }
        ],
        "description": "Price outliers detected in last hour",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 5,
                  "color": "yellow"
                },
                {
                  "value": 20,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Price Submission Rate",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_price_submissions_total{asset=~\"$asset\"}[5m])) by (asset)",
            "legendFormat": "{{asset}} - Submissions/sec",
            "refId": "A"
          }
        ],
        "description": "Rate of price submissions per asset per second",
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Aggregated Prices",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_aggregated_price{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - Price",
            "refId": "A"
          }
        ],
        "description": "Current aggregated price for each asset",
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "type": "table",
        "title": "Validator Participation",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_validator_submissions_total{validator=~\"$validator\"}[1h])) by (validator)",
            "legendFormat": "{{validator}}",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "paw_oracle_validator_reputation_score{validator=~\"$validator\"}",
            "legendFormat": "{{validator}}",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(increase(paw_oracle_missed_votes_total{validator=~\"$validator\"}[24h])) by (validator)",
            "legendFormat": "{{validator}}",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "description": "Validator participation metrics: submission rate, reputation, and missed votes",
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Value #A"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Submissions/hour"
                },
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value #B"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Reputation"
                },
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {
                        "value": null,
                        "color": "red"
                      },
                      {
                        "value": 50,
                        "color": "yellow"
                      },
                      {
                        "value": 80,
                        "color": "green"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value #C"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Missed Votes (24h)"
                },
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 10,
        "type": "graph",
        "title": "Price Deviation by Asset",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_price_deviation_percent{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - {{validator}}",
            "refId": "A"
          },
          {
            "expr": "avg(paw_oracle_price_deviation_percent{asset=~\"$asset\"}) by (asset)",
            "legendFormat": "{{asset}} - Avg Deviation",
            "refId": "B"
          }
        ],
        "description": "Validator price deviation from median by asset",
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 3,
                  "color": "yellow"
                },
                {
                  "value": 10,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "type": "graph",
        "title": "Price Age",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_price_age_seconds{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - Age",
            "refId": "A"
          }
        ],
        "description": "Time since last price update per asset",
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 60,
                  "color": "yellow"
                },
                {
                  "value": 300,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "type": "graph",
        "title": "Slash Events Timeline",
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "increase(paw_oracle_slashing_events_total{validator=~\"$validator\"}[5m])",
            "legendFormat": "{{validator}} - {{reason}}",
            "refId": "A"
          }
        ],
        "description": "Slashing events over time by validator and reason",
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 13,
        "type": "graph",
        "title": "Price Aggregation Rate",
        "gridPos": {
          "x": 0,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_price_aggregations_total{asset=~\"$asset\"}[5m])) by (asset, status)",
            "legendFormat": "{{asset}} - {{status}}",
            "refId": "A"
          }
        ],
        "description": "Price aggregation execution rate by status",
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 14,
        "type": "graph",
        "title": "Aggregation Latency",
        "gridPos": {
          "x": 12,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(paw_oracle_aggregation_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(paw_oracle_aggregation_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(paw_oracle_aggregation_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "description": "Price aggregation processing latency distribution",
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 15,
        "type": "graph",
        "title": "TWAP Values",
        "gridPos": {
          "x": 0,
          "y": 36,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_twap_price{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - TWAP",
            "refId": "A"
          },
          {
            "expr": "paw_oracle_aggregated_price{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - Spot",
            "refId": "B"
          }
        ],
        "description": "Time-weighted average price vs spot price",
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 16,
        "type": "graph",
        "title": "TWAP Window Size",
        "gridPos": {
          "x": 12,
          "y": 36,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_twap_window_seconds{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - Window",
            "refId": "A"
          }
        ],
        "description": "TWAP calculation window size per asset",
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 17,
        "type": "graph",
        "title": "Outlier Detection Frequency",
        "gridPos": {
          "x": 0,
          "y": 44,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_outliers_detected_total{asset=~\"$asset\"}[5m])) by (asset, severity)",
            "legendFormat": "{{asset}} - {{severity}}",
            "refId": "A"
          }
        ],
        "description": "Outlier detection rate by severity level",
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 18,
        "type": "graph",
        "title": "Price Rejections",
        "gridPos": {
          "x": 12,
          "y": 44,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_price_rejections_total{asset=~\"$asset\"}[5m])) by (asset, reason)",
            "legendFormat": "{{asset}} - {{reason}}",
            "refId": "A"
          }
        ],
        "description": "Price submission rejection rate by reason",
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.1,
                  "color": "yellow"
                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 19,
        "type": "graph",
        "title": "Manipulation Detection",
        "gridPos": {
          "x": 0,
          "y": 52,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_manipulation_detected_total{asset=~\"$asset\"}[1h])) by (asset, detection_method)",
            "legendFormat": "{{asset}} - {{detection_method}}",
            "refId": "A"
          }
        ],
        "description": "Price manipulation attempts detected",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 5,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 20,
        "type": "graph",
        "title": "Circuit Breaker Triggers",
        "gridPos": {
          "x": 12,
          "y": 52,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_circuit_breaker_triggers_total[1h])) by (asset, reason)",
            "legendFormat": "{{asset}} - {{reason}}",
            "refId": "A"
          }
        ],
        "description": "Circuit breaker activations by reason",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 5,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 21,
        "type": "graph",
        "title": "IBC Price Feeds - Sent",
        "gridPos": {
          "x": 0,
          "y": 60,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_ibc_prices_sent_total[5m])) by (destination_chain, asset)",
            "legendFormat": "{{destination_chain}} - {{asset}}",
            "refId": "A"
          }
        ],
        "description": "Price updates sent to other chains via IBC",
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 22,
        "type": "graph",
        "title": "IBC Price Feeds - Received",
        "gridPos": {
          "x": 8,
          "y": 60,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(paw_oracle_ibc_prices_received_total[5m])) by (source_chain, asset)",
            "legendFormat": "{{source_chain}} - {{asset}}",
            "refId": "A"
          }
        ],
        "description": "Price updates received from other chains via IBC",
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 23,
        "type": "graph",
        "title": "IBC Timeouts",
        "gridPos": {
          "x": 16,
          "y": 60,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_ibc_timeouts_total[1h])) by (chain, asset)",
            "legendFormat": "{{chain}} - {{asset}}",
            "refId": "A"
          }
        ],
        "description": "IBC price feed timeout events",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 10,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 24,
        "type": "graph",
        "title": "IBC Latency",
        "gridPos": {
          "x": 0,
          "y": 68,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(paw_oracle_ibc_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(paw_oracle_ibc_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(paw_oracle_ibc_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "description": "Cross-chain oracle operation latency",
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 25,
        "type": "graph",
        "title": "Validator Participation Rate",
        "gridPos": {
          "x": 12,
          "y": 68,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "paw_oracle_consensus_participation_rate{asset=~\"$asset\"}",
            "legendFormat": "{{asset}} - Participation %",
            "refId": "A"
          }
        ],
        "description": "Percentage of validators participating in price consensus",
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 80,
                  "color": "green"
                }
              ]
            }
          }
        }
      }
    ]
  }
}
