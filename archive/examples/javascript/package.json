{
  "name": "paw-examples-javascript",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript examples for PAW blockchain",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:basic": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=basic",
    "test:dex": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=dex",
    "test:staking": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=staking",
    "test:governance": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=governance",
    "test:advanced": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=advanced",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "paw",
    "blockchain",
    "cosmos",
    "examples"
  ],
  "dependencies": {
    "@cosmjs/amino": "^0.32.0",
    "@cosmjs/crypto": "^0.32.0",
    "@cosmjs/encoding": "^0.32.0",
    "@cosmjs/proto-signing": "^0.32.0",
    "@cosmjs/stargate": "^0.32.0",
    "@cosmjs/tendermint-rpc": "^0.32.0",
    "axios": "^1.6.0",
    "bip39": "^3.1.0",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0"
  }
}
