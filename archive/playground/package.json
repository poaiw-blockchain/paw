{
  "name": "paw-playground",
  "version": "1.0.0",
  "description": "Interactive developer playground for PAW blockchain",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "http-server -p 8080 -c-1",
    "dev": "http-server -p 8080 -c-1 --cors",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "build": "npm run lint && npm run test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "paw",
    "blockchain",
    "playground",
    "developer",
    "interactive"
  ],
  "author": "PAW Team",
  "license": "MIT",
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "axios": "^1.6.2",
    "codemirror": "^6.0.1",
    "highlight.js": "^11.9.0",
    "js-beautify": "^1.14.11",
    "marked": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
