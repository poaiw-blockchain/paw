{
  "name": "@paw-chain/sdk",
  "version": "1.1.0",
  "description": "Official JavaScript/TypeScript SDK for PAW blockchain (paw-mvp-1 testnet)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "paw",
    "poaiw",
    "blockchain",
    "cosmos",
    "cosmos-sdk",
    "sdk",
    "ibc",
    "staking",
    "governance"
  ],
  "author": "PAW Chain <info@poaiw.org>",
  "license": "Apache-2.0",
  "homepage": "https://docs.poaiw.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/poaiw-blockchain/paw"
  },
  "bugs": {
    "url": "https://github.com/poaiw-blockchain/paw/issues"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/crypto": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@cosmjs/math": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "bip39": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
