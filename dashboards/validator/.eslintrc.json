{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "globals": {
    "ValidatorAPI": "readonly",
    "ValidatorCard": "readonly",
    "DelegationList": "readonly",
    "RewardsChart": "readonly",
    "UptimeMonitor": "readonly",
    "ValidatorWebSocket": "readonly",
    "MockValidatorWebSocket": "readonly",
    "createWebSocketConnection": "readonly",
    "ValidatorDashboard": "readonly",
    "dashboard": "writable"
  }
}
