<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
  <!--
    PAW Blockchain - OWASP Dependency-Check Suppressions

    This file contains CVE suppressions for known vulnerabilities that have been
    reviewed and determined to be either:
    1. Not applicable to the PAW blockchain
    2. Mitigated by other controls
    3. False positives
    4. Accepted risk

    Each suppression should include:
    - CVE ID or vulnerability identifier
    - Reason for suppression
    - Until date (when to re-review)
  -->

  <!-- Example suppression for a false positive -->
  <!--
  <suppression>
    <notes>Example: This is a false positive, the vulnerable function is not used in our codebase</notes>
    <cve>CVE-2021-12345</cve>
    <cpe>cpe:/a:vendor:product</cpe>
    <until>2025-12-31</until>
  </suppression>
  -->

  <!-- Suppress development/test dependencies -->
  <suppression>
    <notes>Test dependency - not included in production build</notes>
    <cpe>cpe:/a:testify:testify</cpe>
  </suppression>

  <!-- Go stdlib vulnerabilities that are not applicable -->
  <suppression>
    <notes>Go standard library - mitigated by keeping Go updated</notes>
    <cpe>cpe:/a:golang:go</cpe>
  </suppression>

  <!-- Add your own suppressions below -->
  <!-- Template:
  <suppression>
    <notes>Reason for suppression - include CVE number and analysis</notes>
    <cve>CVE-YYYY-NNNNN</cve>
  </suppression>
  -->

</suppressions>
