                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 25,
        "type": "timeseries",
        "title": "Manipulation Detection",
        "gridPos": {
          "x": 0,
          "y": 65,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_manipulation_detected_total{asset=~\"$asset\"}[1h])) by (asset, detection_method)",
            "legendFormat": "{{asset}} - {{detection_method}}",
            "refId": "A"
          }
        ],
        "description": "Price manipulation attempts detected by method",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                },
                {
                  "value": 5,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 26,
        "type": "timeseries",
        "title": "Circuit Breaker Triggers",
        "gridPos": {
          "x": 12,
          "y": 65,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(paw_oracle_circuit_breaker_triggers_total[1h])) by (asset, reason)",
            "legendFormat": "{{asset}} - {{reason}}",
            "refId": "A"
          }
        ],
        "description": "Circuit breaker activations by asset and reason",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
