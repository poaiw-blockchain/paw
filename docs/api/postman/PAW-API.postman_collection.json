{
  "info": {
    "name": "PAW Blockchain API",
    "description": "Complete API collection for PAW Blockchain - DEX, Oracle, Compute, and Cosmos SDK endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:1317",
      "type": "string"
    },
    {
      "key": "my_address",
      "value": "paw1abc123...",
      "type": "string"
    },
    {
      "key": "validator_address",
      "value": "pawvaloper1abc...",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "DEX Module",
      "item": [
        {
          "name": "List All Pools",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/pools",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "pools"]
            }
          }
        },
        {
          "name": "Get Pool by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/pools/1",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "pools", "1"]
            }
          }
        },
        {
          "name": "Get Pool Price",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/pools/1/price",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "pools", "1", "price"]
            }
          }
        },
        {
          "name": "Estimate Swap",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"pool_id\": 1,\n  \"token_in\": \"uapaw\",\n  \"amount_in\": \"1000000\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/estimate_swap",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "estimate_swap"]
            }
          }
        },
        {
          "name": "Create Pool",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"creator\": \"{{my_address}}\",\n  \"token_a\": \"uapaw\",\n  \"token_b\": \"ubtc\",\n  \"amount_a\": \"1000000000\",\n  \"amount_b\": \"500000000\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/create_pool",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "create_pool"]
            }
          }
        },
        {
          "name": "Swap Tokens",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"sender\": \"{{my_address}}\",\n  \"pool_id\": 1,\n  \"token_in\": \"uapaw\",\n  \"amount_in\": \"1000000\",\n  \"min_amount_out\": \"450000\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/paw/dex/v1/swap",
              "host": ["{{base_url}}"],
              "path": ["paw", "dex", "v1", "swap"]
            }
          }
        }
      ]
    },
